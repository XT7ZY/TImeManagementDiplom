@model TImeManagement.ViewModels.Account.RegisterViewModel

<form method="post" id="changePasswordForm">
         <div class="form-group col-md-12">
            <label class="col-form-label">Имя</label>
            <input asp-for="@Model.Name" class="form-control">
        </div>
        <div class="form-group col-md-12">
            <label class="col-form-label">Фамилия</label>
            <input asp-for="@Model.LastName" class="form-control">
        </div>
        <div class="form-group col-md-12">
            <label class="col-form-label">Отчество</label>
            <input asp-for="@Model.SurName" class="form-control">
        </div>
        <div class="form-group col-md-12">
            <label class="col-form-label">Логин</label>
            <input  class="form-control" asp-for="@Model.Login"/>
        </div>
        <div class="form-group col-md-12">
            <label class="col-form-label">Роль</label>
            <input  class="form-control" asp-for="@Model.RoleID"/>
        </div>
        <div class="form-group col-md-12">
            <label class="col-form-label">Пароль</label>
            <input  class="form-control" asp-for="@Model.Password"/>
        </div>
        <div class="form-group col-md-12">
            <label class="col-form-label">Подтвердите пароль</label>
            <input asp-for="@Model.PasswordConfirm"  class="form-control">
        </div>
        <div class="modal-footer">
             <button class="btn btn-primary" asp-controller="Account" asp-action="Register" type="submit">Добавить пользователя</button>
        </div>
</form>